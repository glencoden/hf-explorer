{
    "name": "hf-json-explorer",
    "version": "1.0.0",
    "scripts": {
        "build": "tailwindcss -i src/tailwind.css -o dist/main.css --minify && esbuild src/index.tsx --bundle --outfile=dist/index.js && cp preview/index.html dist/index.html",
        "dev:tailwind": "tailwindcss -i src/tailwind.css -o dist/main.css --watch",
        "dev:esbuild": "pnpm build --watch",
        "dev": "run-p dev:*",
        "lint": "prettier --write src/**/*.{ts,tsx}"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "zustand": "5.0.0-rc.2"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.9",
        "@types/node": "^22.7.5",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "autoprefixer": "^10.4.20",
        "esbuild": "^0.24.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "tailwindcss": "^3.4.13",
        "typescript": "^5.6.3"
    }
}
